{{!-- Dashboard View --}}
<h1>Your Dashboard</h1>
<button id="new-post">Create New Post</button>

{{#each posts}}
    <div class="post">
        <h2>{{this.title}}</h2>
        <p>{{this.content}}</p>
        <button class="edit-post" data-id="{{this.id}}">Edit</button>
        <button class="delete-post" data-id="{{this.id}}">Delete</button>
    </div>
{{/each}}

<script src="/js/editPost.js"></script>
